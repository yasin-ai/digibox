        </ul>

        <p>Welcome</p>
      </div>
    </nav>

    <div class="container">
      <div class="card">
        <h2>Upload details</h2>
        <br>
 
        <form action="/candidate/profile" method="POST" enctype="multipart/form-data">
            <div class="form-group row">
              <label for="username-input" class="col-sm-4 col-form-label" name="username" required>Username</label>
              <div class="col-sm-8">
                <input type="text" class="form-control" id="username-input" name="username" required>
              </div>
            </div>
            <div class="form-group row">
              <label for="name-input" class="col-sm-4 col-form-label">Full Name</label>
              <div class="col-sm-8">
                <input type="text" class="form-control" id="name-input" name="name" required>
              </div>
            </div>
            <div class="form-group row">
              <label for="age-input" class="col-sm-4 col-form-label">Age</label>
              <div class="col-sm-8">
                <input type="text" class="form-control" id="age-input" name="age" required>
              </div>
            </div>
          
            <label for="profile-photo-input">Profile Photo</label>
            <input type="file" class="form-control-file" id="profile-photo-input" name="profile" accept=".jpg" required>
            <br>
            <label for="resume-input">Resume</label>
            <input type="file" class="form-control-file" id="resume-input" name="resume" accept=".pdf" required>
          
            <div class="mbtn-submit">
              <input type="submit" value="Submit">
            </div>
          </form>
          
      </div>
    </div>

        </div>
        
  </body>
